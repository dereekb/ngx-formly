{
  "name": "@ngx-formly/material",
  "version": "7.1.0",
  "license": "MIT",
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "peerDependencies": {
    "@ngx-formly/core": "7.1.0",
    "@angular/material": ">=16.0.0"
  },
  "module": "fesm2022/ngx-formly-material.mjs",
  "typings": "types/ngx-formly-material.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./types/ngx-formly-material.d.ts",
      "default": "./fesm2022/ngx-formly-material.mjs"
    },
    "./checkbox": {
      "types": "./types/ngx-formly-material-checkbox.d.ts",
      "default": "./fesm2022/ngx-formly-material-checkbox.mjs"
    },
    "./datepicker": {
      "types": "./types/ngx-formly-material-datepicker.d.ts",
      "default": "./fesm2022/ngx-formly-material-datepicker.mjs"
    },
    "./form-field": {
      "types": "./types/ngx-formly-material-form-field.d.ts",
      "default": "./fesm2022/ngx-formly-material-form-field.mjs"
    },
    "./input": {
      "types": "./types/ngx-formly-material-input.d.ts",
      "default": "./fesm2022/ngx-formly-material-input.mjs"
    },
    "./multicheckbox": {
      "types": "./types/ngx-formly-material-multicheckbox.d.ts",
      "default": "./fesm2022/ngx-formly-material-multicheckbox.mjs"
    },
    "./native-select": {
      "types": "./types/ngx-formly-material-native-select.d.ts",
      "default": "./fesm2022/ngx-formly-material-native-select.mjs"
    },
    "./radio": {
      "types": "./types/ngx-formly-material-radio.d.ts",
      "default": "./fesm2022/ngx-formly-material-radio.mjs"
    },
    "./select": {
      "types": "./types/ngx-formly-material-select.d.ts",
      "default": "./fesm2022/ngx-formly-material-select.mjs"
    },
    "./slider": {
      "types": "./types/ngx-formly-material-slider.d.ts",
      "default": "./fesm2022/ngx-formly-material-slider.mjs"
    },
    "./textarea": {
      "types": "./types/ngx-formly-material-textarea.d.ts",
      "default": "./fesm2022/ngx-formly-material-textarea.mjs"
    },
    "./toggle": {
      "types": "./types/ngx-formly-material-toggle.d.ts",
      "default": "./fesm2022/ngx-formly-material-toggle.mjs"
    }
  },
  "sideEffects": false,
  "homepage": "https://github.com/ngx-formly/ngx-formly#readme",
  "description": "Formly is a dynamic (JSON powered) form library for Angular that bring unmatched maintainability to your application's forms.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngx-formly/ngx-formly.git"
  },
  "bugs": {
    "url": "https://github.com/ngx-formly/ngx-formly/issues"
  }
}