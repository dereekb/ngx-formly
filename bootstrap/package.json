{
  "name": "@ngx-formly/bootstrap",
  "version": "7.1.0",
  "license": "MIT",
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "peerDependencies": {
    "@ngx-formly/core": "7.1.0",
    "bootstrap": "^5.0.0"
  },
  "module": "fesm2022/ngx-formly-bootstrap.mjs",
  "typings": "types/ngx-formly-bootstrap.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./types/ngx-formly-bootstrap.d.ts",
      "default": "./fesm2022/ngx-formly-bootstrap.mjs"
    },
    "./addons": {
      "types": "./types/ngx-formly-bootstrap-addons.d.ts",
      "default": "./fesm2022/ngx-formly-bootstrap-addons.mjs"
    },
    "./checkbox": {
      "types": "./types/ngx-formly-bootstrap-checkbox.d.ts",
      "default": "./fesm2022/ngx-formly-bootstrap-checkbox.mjs"
    },
    "./form-field": {
      "types": "./types/ngx-formly-bootstrap-form-field.d.ts",
      "default": "./fesm2022/ngx-formly-bootstrap-form-field.mjs"
    },
    "./input": {
      "types": "./types/ngx-formly-bootstrap-input.d.ts",
      "default": "./fesm2022/ngx-formly-bootstrap-input.mjs"
    },
    "./multicheckbox": {
      "types": "./types/ngx-formly-bootstrap-multicheckbox.d.ts",
      "default": "./fesm2022/ngx-formly-bootstrap-multicheckbox.mjs"
    },
    "./radio": {
      "types": "./types/ngx-formly-bootstrap-radio.d.ts",
      "default": "./fesm2022/ngx-formly-bootstrap-radio.mjs"
    },
    "./select": {
      "types": "./types/ngx-formly-bootstrap-select.d.ts",
      "default": "./fesm2022/ngx-formly-bootstrap-select.mjs"
    },
    "./textarea": {
      "types": "./types/ngx-formly-bootstrap-textarea.d.ts",
      "default": "./fesm2022/ngx-formly-bootstrap-textarea.mjs"
    }
  },
  "sideEffects": false,
  "homepage": "https://github.com/ngx-formly/ngx-formly#readme",
  "description": "Formly is a dynamic (JSON powered) form library for Angular that bring unmatched maintainability to your application's forms.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngx-formly/ngx-formly.git"
  },
  "bugs": {
    "url": "https://github.com/ngx-formly/ngx-formly/issues"
  }
}